{
    "compilerOptions": {
        "sourceMap": true,
        "allowJs": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "forceConsistentCasingInFileNames": true,
        "alwaysStrict": true,
        "noUnusedParameters": true,
        "lib": [
            "es5",
            "es6",
            "dom",
            "es2015",
            "esnext.asynciterable"
        ]
    }
}
